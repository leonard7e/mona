<section id="itsBanner" ng-controller="Banner">
  <header id="mainHeader">
    <div class="header">
      <div
        class="Sandwich"
        ng-init="init()"
        role=button
        ng-click="toggleSandwich()"
        ng-show="doSandwich"
        ng-cloak
        tabindex="0"
        >
        &#9776;
      </div>
      <a href="{{ base_url }}">
        <img src="{{ theme_url}}/images/shnw_banner.svg" alt="{{ config.site.title }}" />
      </a>
    </div>
  </header>
  <div class="navSplit" ng-show="sandwichMenuOpened" > </div>
  {% include 'partials/navigation/main_navigation.html.twig' %}
</section>
